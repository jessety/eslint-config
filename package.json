{
  "name": "@jessety/eslint-config",
  "version": "2.2.2",
  "description": "ESLint config for TypeScript or JavaScript by @jessety",
  "keywords": [
    "eslint",
    "eslint-config",
    "lint",
    "linter",
    "linting"
  ],
  "homepage": "https://github.com/jessety/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/jessety/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jessety/eslint-config.git"
  },
  "license": "MIT",
  "author": "Jesse Youngblood <jessetyoungblood@gmail.com> (https://jesseyoungblood.com)",
  "main": ".eslintrc.js",
  "scripts": {
